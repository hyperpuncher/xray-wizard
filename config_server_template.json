{
	"log": {
		"loglevel": "debug"
	},
	"inbounds": [
		{
			"port": 443,
			"protocol": "vless",
			"settings": {
				"clients": [],
				"decryption": "none"
			},
			"streamSettings": {
				"network": "raw",
				"security": "reality",
				"realitySettings": {
					"dest": "1.1.1.1:443",
					"serverNames": [""],
					"privateKey": "",
					"shortIds": ["", "0123456789abcdef"]
				}
			},
			"sniffing": {
				"enabled": true,
				"destOverride": ["http", "tls", "quic"],
				"routeOnly": true
			}
		}
	],
	"outbounds": [
		{
			"protocol": "freedom",
			"tag": "direct"
		}
	],
	"dns": {
		"servers": ["https+local://1.1.1.1/dns-query", "https+local://1.0.0.1/dns-query"],
		"queryStrategy": "UseIPv4"
	}
}
